<% if current_user %>
Signed in as <strong><%= current_user.email %></strong>
<div id="items" class=“transitions-enabled”>
    <% @items.each do |item| %>
    <div class="box panel panel-default">
        <h2><%= image_tag item.image_url %></h2>
    <div class="panel-body">
      <h2><%= link_to item.title, item %></h2>
      <p class="user">Submitted by:  </p>
    </div>
    </div>
    <% end %>
    <div class="friendships">
      <div class="row">
        <li><%= link_to 'Friends' friendship_path %></li>
        <li><%= link_to "Find Friends", users_path %></li>
      </div>
    </div>
    <% end %>
</div>



