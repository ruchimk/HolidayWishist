
<%= form_for @item do |f| %>
  <%= f.text_field :title, placeholder: "Item Title" %>
  <%= f.text_field :description, placeholder: "Description" %>
  <%= f.text_field :image_url, placeholder: "Image URL" %>
  <%= f.text_field :url, placeholder: "URL" %>
  Private?  <%= f.check_box :is_private %>
  <%= f.submit 'Add Item'%>
<% end %>
